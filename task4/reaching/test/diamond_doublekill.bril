# Block 0 reaching definitions:
# Block 1 (ge) reaching definitions:
#   From block 0 : var b instr 0
#   From block 0 : var cond0 instr 1
# Block 2 (zero) reaching definitions:
#   From block 0 : var b instr 0
#   From block 0 : var cond0 instr 1
# Block 3 (end) reaching definitions: 
#   From block 0 : var cond0 instr 1
#   From block 1 : var b instr 1
#   From block 2 : var b instr 1

@main (input: int) {
    b: int = const one;
    cond0: bool = ge input b;
    br cond0 .ge .zero;
.ge:
    b: int = sub input one;
    jmp .end;
.zero:
    b: int = const input;
    jmp. end;
.end:
    ret;
}