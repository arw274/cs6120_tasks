# Block 0 reaching definitions:
# Block 1 reaching definitions:
# Block 2 (ge) reaching definitions:
#   From block 0 : var one instr 0
#   From block 0 : var cond0 instr 1
#   From block 1 : var b instr 0
# Block 3 (zero) reaching definitions: 
#   From block 0 : var one instr 0
#   From block 0 : var cond0 instr 1
# Block 4 (end) reaching definitions: 
#   From block 0 : var one instr 0
#   From block 0 : var cond0 instr 1
#   From block 1 : var b instr 0
#   From block 2 : var input instr 1
#   From block 3 : var input instr 1

@main (input: int) {
    one: int = const 1;
    cond0: bool = ge input one;
    br cond0 .ge .zero;
    b: int = const one;
.ge:
    input: int = sub input one;
    jmp .end;
.zero:
    input: int = const input;
    jmp .end;
.end:
    ret;
}