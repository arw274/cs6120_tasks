# main: 0 -> {1}, 1 -> {3, 4}, 2 -> {3}, 3 -> {1}, 4 -> {}
# func : 0 -> {}
@main (input: int) {
    one: int = const 1;
    cond0: bool = ge input one;
.loop:
    br cond0 .ge .zero;
    b: int = const one;
.ge:
    input: int = call @func input;
    jmp .loop;
.zero:
    ret;
}

@func (input: int) : int {
    input: int = sub input one;
    ret input;
}